       //hadap dan gerak ke kiri
       sprite_index = pacman_left_spr;  
       global.gerak = 1;
   }
   //bawah
   else if ((global.panah==4) &amp;&amp; !place_meeting(x, y+3, wall_obj)) {
       //hadap dan gerak ke bawah
       sprite_index = pacman_down_spr;  
       global.gerak = 4;
   }


    
   
   //lakukan gerakan
   if (global.gerak == 2) {
        y = y - 2; //atas
   } else  if (global.gerak==3) {
        x = x + 2; //kanan
   } else  if (global.gerak==1) {
        x = x - 2; //kiri 
   } else  if (global.gerak==4) {
        y = y + 2; //bawah
   }  
   



</string>
